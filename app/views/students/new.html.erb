 <h1>Create New HomeSchool Profile</h1>
 <%= form_tag students_path do %>
  <label>Child's Name:</label>
  <%= text_field_tag :'student[name]' %><br>
 
  <label>Child's Age:</label>
  <%= text_field_tag :'student[age]' %><br>

  <label>Child's Grade:</label>
  <%= text_field_tag :'student[grade_level]' %><br>
 
  <%= submit_tag "Create Profile" %>
<% end %>