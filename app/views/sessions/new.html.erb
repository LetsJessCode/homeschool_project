<h1>Welcome Back</h1>
<hr>
    <h4>Please Login</h4>
 <hr>
<%= form_for @user, url: login_path do |f| %> <!-- ArgumentError in Sessions#create-->

<div>   
    <%= f.label :email %>
    <%= f.text_field :email %>
</div>
<div>   
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>

    <%=f.submit "Login" %>
<% end %> 